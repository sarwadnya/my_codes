void del_loop(Node*head)
{
  Node*s=head,*f=head;
  while(f && f->next)
  {
    s=s->next;
    f=f->next->next;
    if(s==f)
      break;
  }
  if(s!=f)  
    return;
  if(s==head)
  {
    while(s->next!=head)
    {
      s=s->next;
    }
    s->next=NULL;
    return;
  }
  f=head;
  while(s->next!=f->next)
  {
    s=s->next;
    f=f->next;
  }
  s->next=NULL;
  return;
}
