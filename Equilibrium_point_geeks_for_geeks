#include <stdio.h>
#include <stdlib.h>
#include <limits.h>
int main (int argc, char *argv[])
{
	int t;
	scanf("%d",&t);
	while (t--)
	{
		int flag=0;
		int n;
		scanf("%d",&n);
		int arr[n];
		for (int i = 0; i < n; i += 1)
		{
			scanf("%d",&arr[i]);
		}
		int arr1[n],arr2[n];
		for (int i = 0; i < n; i += 1)
		{
			arr1[i]=arr[i];
			arr2[i]=0;
		}
		for (int i = 1; i < n; i += 1)
		{
			arr2[i]+=arr1[i-1];
			arr1[i]+=arr1[i-1];
		}
		for (int i = n-2; i >= 0; i -= 1)
		{
			arr2[i]-=arr[i+1];
			arr[i]+=arr[i+1];
		}
		for (int i = 0; i < n; i += 1)
		{
			if (arr2[i]==0)
			{
				printf("%d\n",i+1);
				flag=1;
				break;
			}
		}
		if (!flag)
		{
			printf("-1\n");
		}
	}
	return 0;
}
