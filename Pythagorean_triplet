#include <stdio.h>
#include <stdlib.h>
#include <limits.h>
int num1;
int num2;
void find_trip_odd(int a)
{
	int s=a*a;
	s/=2;
	num1=s;
	num2=s+1;
	return;
}
void find_trip_even(int a)
{
	int s=a*a;
	s/=4;
	num1=s-1;
	num2=s+1;
	return;
}
int main (int argc, char *argv[])
{
	int t;
	scanf("%d",&t);
	while (t--)
	{
		int n;
		scanf("%d",&n);
		int arr[n];
		int max=INT_MIN;
		for (int i = 0; i < n; i += 1)
		{
			scanf("%d",&arr[i]);
			if (max<arr[i])
			{
				max=arr[i];
			}
		}
		int flag=1;
		for (int i = 0; i < n; i += 1)
		{
			if (arr[i]%2)
			{
				find_trip_odd(arr[i]);
			}
			else
			{
				find_trip_even(arr[i]);
			}
			printf("%d %d\n",num1,num2);
			int flag1=0;
			int flag2=0;
			for (int j = 0;j!=i && j < n; j += 1)
			{
				if (arr[j]==num1)
				{
					flag1=1;
				}
			}
			for (int j = 0; j < n; j += 1)
			{
				if (arr[j]==num2)
				{
					flag2=1;
				}
			}
			if (flag1 && flag2)
			{
				flag=0; 
				printf("YES\n");
				break;
			}
		}
		if (flag)
		{
			printf("NO\n");
		}
	}
	return 0;
}
